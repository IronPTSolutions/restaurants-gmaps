<table class="table table-striped table-hover">
 <thead>
   <tr>
     <th></th>
     <th>Name</th>
     <th>Description</th>
     <th>Lat</th>
     <th>Lng</th>
     <th></th>
   </tr>
 </thead>
 <tbody>
   <% restaurants.forEach((restaurant) => { %>
   <tr>
     <td>
       <img src="uploads/<%= restaurant.file %>" alt="restaurant image">
     </td>
     <td><%= restaurant.name %></td>
     <td><%= restaurant.description %></td>
     <td><%= restaurant.location.coordinates[0] %></td>
     <td><%= restaurant.location.coordinates[1] %></td>
     <td>
       <form action="/restaurants/<%= restaurant._id %>/delete" method="post">
         <input type="submit" value="Delete" class="Delete">
       </form>
       <br>
       <form action="/restaurants/edit/<%= restaurant._id %>" method="post">
         <input type="submit" value="Edit" class="Edit">
       </form>
     </td>
   </tr>
 <% }) %>
 </tbody>
</table>
